{
  "metadata": {
      "name": "${appName}"
  },
  "projects": [
      {
          "name": "${appName}",
          "source": {
              "location": "https://github.com/kevbrain/${appName}.git",
              "type": "github"
          }
      }
  ],
  "components": [
      {
          "id": "redhat/vscode-xml/latest",
          "type": "chePlugin"
      },
      {
          "id": "redhat/java/latest",
          "preferences": {
              "java.jdt.ls.vmargs": "-javaagent:/lombok.jar"
          },
          "type": "chePlugin"
      },
      {
          "id": "redhat/vscode-yaml/latest",
          "type": "chePlugin"
      }
  ],
"apiVersion": "1.0.0"
}